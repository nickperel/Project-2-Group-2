<link rel="stylesheet" href="/CSS/footer.css">

<h1 class="login-title">Already Have an Account? Log in Here!</h1>

<div class="w-60 mx-auto">

  <form class="login-text login-form">
  <div class="mb-3 login-card">
    <h2 class="page-title">Log in</h2>
    <label for="email-login" class="form-label">Email address</label>
    <input type="email" class="form-control" id="email-login" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  </div>
  <div class="mb-3">
    <label for="password-login" class="form-label">Password</label>
    <input type="password" class="form-control" id="password-login">
  </div>
  <button type="submit" class="btn btn-success">Login</button>
  </form>
  
<a class="btn btn-danger home-btn mt-3"href="/signup">Sign Up Here</a>
</div>


<script src="./js/login.js"></script>